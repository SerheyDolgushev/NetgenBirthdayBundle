{
    "name": "netgen/birthday-bundle",
    "description": "Netgen Birthday bundle for dates before 1970 in eZ Publish 5",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "ezsystems/ezpublish-kernel": "^6.0|^7.0",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "netgen/ez-forms-bundle": "*"
    },
    "suggest": {
        "netgen/birthday": "To edit this field type in legacy administration interface",
